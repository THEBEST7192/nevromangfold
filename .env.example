# Backend (Express) configuration
DB_HOST=your_database_host
DB_PORT=your_database_port (By default 3306)
DB_USER=your_username
DB_PASSWORD=your_password
DB_NAME=your_database_name
CLIENT_ORIGIN=http://localhost:5173
PORT=your_port

# Cloudflare Access (optional - only needed when DB is behind Cloudflare Zero Trust)
CF_ACCESS_CLIENT_ID=your_cloudflare_access_client_id
CF_ACCESS_CLIENT_SECRET=your_cloudflare_access_client_secret

# Frontend (Vite) configuration
VITE_API_BASE_URL=http://localhost:3001
